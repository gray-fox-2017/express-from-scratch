<% include ../partials/header %>

  <div class="">
    <div class="">
      <a href="#"></a>
      <hr>
        <table class="responsive-table">
          <thead>
            <tr>
              <th>ID</th>
              <th>Name</th>
              <th>Hobby</th>
              <th>User Name</th>
              <th>email</th>
            </tr>
          </thead>
          <tbody>
            <%
              data.forEach((user)=>{
              %>
              <tr>
                <td><%= user.id %></td>
                <td><%= user.name %></td>
                <td><%= user.hobby %></td>
                <td><%= user.username %></td>
                <td><%= user.email %></td>
                <a href="/users/edit/<%= user.id %>"  class="waves-effect waves-light btn"><i class="material-icons">edit</i></a> | <a href="/users/delete/<%= user.id %>" class="btn red" onclick="return confirm('Are you sure you want to delete this item?');"><i class="material-icons">delete</i></a>
              </tr>
              <%
              })
              %>
          </tbody>
        </table>
      </div>
    </div>

<% include ../partials/footer %>
